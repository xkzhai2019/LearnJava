public class Archiver {
	/**
	 * 创建归档文件
	 */
	public void newArachiverFile(String[] paths,String yarPath){
		try {
			FileOutputStream fout = new FileOutputStream(yarPath);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		
	}
}
