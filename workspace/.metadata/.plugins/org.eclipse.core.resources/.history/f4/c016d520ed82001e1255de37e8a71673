public class CopyThread extends Thread{
	private String srcFile;
	private String destDir;
	
	private int start;
	private int end;

	public CopyThread(String srcFile, String destDir, int start, int end) {
		this.srcFile = srcFile;
		this.destDir = destDir;
		this.start = start;
		this.end = end;
	}



	@Override
	public void run() {
	}
}
