package com.xkzhai.string;

public class App2 {
	public static void main(String[] args) throws Exception{
		String str = "a中b";
		
		// 编码
		byte[] bytes = str.getBytes("GBK");
		System.out.println(bytes.length);
		
		// 解码，格式要一致
		String str2 = new String(bytes,"GBK");
		System.out.println(str2);
	
	}
}
