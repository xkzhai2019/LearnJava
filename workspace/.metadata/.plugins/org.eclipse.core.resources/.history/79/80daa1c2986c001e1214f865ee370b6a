package com.xkzhai.mulcopier;

import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JTextField;

/**
 * 主窗口
 * @author Administrator
 *
 */
public class CopyUI extends JFrame{

	private static final long serialVersionUID = 6189847546972653742L;
	private JLabel lblSrcFile;
	public CopyUI(){
		init();
	}
	private void init() {
		this.setTitle("主窗口");
		this.setBounds(100, 100, 800, 600);
		this.setLayout(null);
		
		// srcFile标签
		lblSrcFile = new JLabel("源文件");
		lblSrcFile.setBounds(10, 10, 50, 30);
		this.add(lblSrcFile);
		
		JTextField tfSrcFile = new JTextField();
		tfSrcFile.setBounds(60, 10, 400, 30);
		this.add(tfSrcFile);
		
		this.setVisible(true);
	}
}
